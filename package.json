{
  "name": "quirk-bot",
  "version": "2.0.0",
  "private": true,
  "engines": {
    "node": ">=18.15.0"
  },
  "description": "Quirk bot, desenvolvido nas aulas da saga: desenvolvendo um bot de WhatsApp do zero.",
  "main": "./src/start.ts",
  "scripts": {
    "prebuild": "prisma generate && prisma migrate deploy",
    "build:2-server": "tsc",
    "build": "run-s build:*",
    "dev:app": "cross-env NODE_ENV=development tsx watch src/start.ts",
    "dev:pair": "cross-env NODE_ENV=development MODE_PAIR=true tsx watch src/start.ts",
    "dev": "run-s db-up db-migrate-dev db-seed dev:app",
    "db-up": "docker-compose up -d",
    "db-seed": "prisma db seed",
    "db-migrate-dev": "prisma migrate dev",
    "db-nuke": "docker-compose down --volumes --remove-orphans",
    "dx": "run-s db-up db-migrate-dev db-seed",
    "dev-nuke": "rm -rf prisma/*.db**",
    "generate": "prisma generate",
    "migrate-dev": "prisma migrate dev",
    "migrate": "prisma migrate deploy",
    "start": "run-s db-up build start:pm2",
    "start:pm2": "pm2 start ecosystem.config.js --env production && pm2 logs",
    "studio": "prisma studio",
    "lint": "eslint --cache src",
    "lint-fix": "eslint --cache src --fix",
    "test-dev": "cross-env APP_ENV=test start-server-and-test dev 3000 test",
    "test": "playwright test",
    "test-all": "run-s test-dev test",
    "lint-cache-clean": "rm -rf .eslintcache",
    "postinstall": "prisma generate"
  },
  "keywords": [],
  "author": "Gabriel Oliveira",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.1.1",
    "@squarecloud/api": "^3.0.3",
    "@types/node-cache": "^4.1.3",
    "axios": "^1.4.0",
    "baileys": "^6.7.16",
    "child_process": "^1.0.2",
    "dotenv": "^16.4.5",
    "node-cache": "^5.1.2",
    "pino": "^9.4.0",
    "pino-pretty": "^11.2.2",
    "pm2": "^5.4.1",
    "uuid": "^10.0.0"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all",
    "singleQuote": true
  },
  "devDependencies": {
    "@gradio/client": "^0.17.0",
    "@types/node-fetch": "^2.6.4",
    "@types/pino": "^7.0.5",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "betabotz-tools": "^0.0.7",
    "correios-brasil": "^3.0.6",
    "cross-env": "^7.0.3",
    "eslint": "^8.47.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "npm-run-all": "^4.1.5",
    "prisma": "^5.22.0",
    "qrcode-terminal": "^0.12.0",
    "tsx": "^4.19.2",
    "typescript": "^5.1.6"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
