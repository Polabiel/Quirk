generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model fatos {
  id      Int     @id @default(autoincrement())
  fato    String  @unique
  criador String?
}

model user {
  number    String        @id @unique
  name      String
  blacklist Boolean       @default(false)
  sugger    suggestions[]
}

model suggestions {
  id       Int    @id @default(autoincrement())
  sugestao String @unique

  userNumber String
  user       user   @relation(fields: [userNumber], references: [number])
}

model group {
  number        String  @id @unique
  enable        Boolean @default(false)
  blacklist     Boolean @default(false)
  TOKEN_OPEANAI String? @unique
}

model links {
  id      Int     @id @default(autoincrement())
  link    String  @unique
  criador String?
}

model stickers {
  id          Int    @id @default(autoincrement())
  url_sticker String
  name        String
  criador     String
}
